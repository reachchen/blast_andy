<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">
            <solid android:color="@color/gray_6c6c6c" />
            <stroke
                android:color="@color/gray_6c6c6c" />
            <size
                android:width="@dimen/y155"
                android:height="@dimen/y50"/>

            <corners android:radius="@dimen/y50"/>
        </shape>
    </item>
    <item
        android:bottom="@dimen/y5"
        android:left="@dimen/y5"
        android:right="@dimen/y110"
        android:top="@dimen/y5">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <stroke
                android:color="@color/white" />
            <size
                android:width="@dimen/y40"
                android:height="@dimen/y35" />
            <padding android:left="@dimen/y50" android:right="@dimen/x10" />
        </shape>
    </item>
</layer-list>